
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /leads_v2/{leadId} {
      allow read, write: if false; // Default deny
    }

    // Allow logged-in users to read from the leads_v2 collection
    match /leads_v2/{leadId} {
       allow read, list: if true;
    }
  }
}
